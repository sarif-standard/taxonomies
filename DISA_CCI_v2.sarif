          "guid": "A5D61558-0042-4F62-A2E3-59EA163BD5D6",
          "name": "DISA CCI V2",
          "organization": "DISA Field Security Operations",
          "shortDescription": {
            "text": "DISA Control Correlation Identifier (CCI) Version 2"
          },
          "version": "2",
          "releaseDateUtc": "2011-02-28",
          "downloadUri": "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/u_cci_list.zip",
          "informationUri": "https://dl.dod.cyber.mil/wp-content/uploads/stigs/zip/u_draft_cci_specification_v2r0.2.zip",
          "taxa": [
            {
              "id": "CCI-000001",
              "shortDescription": {
                "text": "The organization develops an access control policy that addresses purpose, scope, roles, responsibilities, management commitment, coordination among organizational entities, and compliance."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-1",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000002",
              "shortDescription": {
                "text": "The organization disseminates the access control policy to organization-defined personnel or roles."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-1",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000003",
              "shortDescription": {
                "text": "The organization reviews and updates the access control policy in accordance with organization-defined frequency."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-1",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000004",
              "shortDescription": {
                "text": "The organization develops procedures to facilitate the implementation of the access control policy and associated access controls."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-1",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000005",
              "shortDescription": {
                "text": "The organization disseminates the procedures to facilitate access control policy and associated access controls to the organization-defined personnel or roles."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-1",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000006",
              "shortDescription": {
                "text": "The organization reviews and updates the access control procedures in accordance with organization-defined frequency."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-1",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000007",
              "shortDescription": {
                "text": "The organization manages information system accounts by identifying account types (i.e., individual, group, system, application, guest/anonymous, and temporary)."
              }
            },
            {
              "id": "CCI-000008",
              "shortDescription": {
                "text": "The organization establishes conditions for group membership."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000009",
              "shortDescription": {
                "text": "The organization manages information system accounts by identifying authorized users of the information system and specifying access privileges."
              }
            },
            {
              "id": "CCI-000010",
              "shortDescription": {
                "text": "The organization requires approvals by organization-defined personnel or roles for requests to create information system accounts."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000011",
              "shortDescription": {
                "text": "The organization creates, enables, modifies, disables, and removes information system accounts in accordance with organization-defined procedures or conditions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000012",
              "shortDescription": {
                "text": "The organization reviews information system accounts for compliance with account management requirements per organization-defined frequency."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000013",
              "shortDescription": {
                "text": "The organization manages information system accounts by notifying account managers when temporary accounts are no longer required and when information system users are terminated, transferred, or information system usage or need-to-know/need-to-share changes."
              }
            },
            {
              "id": "CCI-000014",
              "shortDescription": {
                "text": "The organization manages information system accounts by granting access to the system based on a valid access authorization; intended system usage; and other attributes as required by the organization or associated missions/business functions."
              }
            },
            {
              "id": "CCI-000015",
              "shortDescription": {
                "text": "The organization employs automated mechanisms to support the information system account management functions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000016",
              "shortDescription": {
                "text": "The information system automatically removes or disables temporary accounts after an organization-defined time period for each type of account."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000017",
              "shortDescription": {
                "text": "The information system automatically disables inactive accounts after an organization-defined time period."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000018",
              "shortDescription": {
                "text": "The information system automatically audits account creation actions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000019",
              "shortDescription": {
                "text": "The organization requires that users log out in accordance with the organization-defined time period of inactivity or description of when to log out."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-2",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000020",
              "shortDescription": {
                "text": "The information system dynamically manages user privileges and associated access authorizations."
              }
            },
            {
              "id": "CCI-000021",
              "shortDescription": {
                "text": "The information system enforces dual authorization for organization-defined privileged commands and/or other organization-defined actions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-3",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000022",
              "shortDescription": {
                "text": "The information system enforces one or more organization-defined nondiscretionary access control policies over an organization-defined set of users and resources."
              }
            },
            {
              "id": "CCI-000023",
              "shortDescription": {
                "text": "The organization develops an organization-wide information security program plan that provides sufficient information about the program management controls and common controls (including specification of parameters for any assignment and selection operations either explicitly or by reference) to enable an implementation that is unambiguously compliant with the intent of the plan, and a determination of the risk to be incurred if the plan is implemented as intended."
              }
            },
            {
              "id": "CCI-000024",
              "shortDescription": {
                "text": "The information system prevents access to organization-defined security-relevant information except during secure, non-operable system states."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-3",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000025",
              "shortDescription": {
                "text": "The information system enforces information flow control using explicit security attributes on information, source, and destination objects as a basis for flow control decisions."
              }
            },
            {
              "id": "CCI-000026",
              "shortDescription": {
                "text": "The information system uses protected processing domains to enforce organization-defined information flow control policies as a basis for flow control decisions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000027",
              "shortDescription": {
                "text": "The information system enforces dynamic information flow control based on organization-defined policies."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000028",
              "shortDescription": {
                "text": "The information system prevents encrypted information from bypassing content-checking mechanisms by employing organization-defined procedures or methods."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000029",
              "shortDescription": {
                "text": "The information system enforces organization-defined limitations on the embedding of data types within other data types."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000030",
              "shortDescription": {
                "text": "The information system enforces information flow control based on organization-defined metadata."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000031",
              "shortDescription": {
                "text": "The information system enforces organization-defined one-way flows using hardware mechanisms."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000032",
              "shortDescription": {
                "text": "The information system enforces information flow control using organization-defined security policy filters as a basis for flow control decisions for organization-defined information flows."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000033",
              "shortDescription": {
                "text": "The information system enforces the use of human review for organization-defined security policy filters when the system is not capable of making an information flow control decision."
              }
            },
            {
              "id": "CCI-000034",
              "shortDescription": {
                "text": "The information system provides the capability for a privileged administrator to enable/disable organization-defined security policy filters under organization-defined conditions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000035",
              "shortDescription": {
                "text": "The information system provides the capability for privileged administrators to configure the organization-defined security policy filters to support different security policies."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-4",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000036",
              "shortDescription": {
                "text": "The organization separates organization-defined duties of individuals."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-5",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000037",
              "shortDescription": {
                "text": "The organization implements separation of duties through assigned information system access authorizations."
              }
            },
            {
              "id": "CCI-000038",
              "shortDescription": {
                "text": "The organization explicitly authorizes access to organization-defined security functions and security-relevant information."
              }
            },
            {
              "id": "CCI-000039",
              "shortDescription": {
                "text": "The organization requires that users of information system accounts or roles, with access to organization-defined security functions or security-relevant information, use non-privileged accounts, or roles, when accessing nonsecurity functions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-6",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000040",
              "shortDescription": {
                "text": "The organization audits any use of privileged accounts, or roles, with access to organization-defined security functions or security-relevant information, when accessing other system functions."
              }
            },
            {
              "id": "CCI-000041",
              "shortDescription": {
                "text": "The organization authorizes network access to organization-defined privileged commands only for organization-defined compelling operational needs."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-6",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000042",
              "shortDescription": {
                "text": "The organization documents the rationale for authorized network access to organization-defined privileged commands in the security plan for the information system."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-6",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000043",
              "shortDescription": {
                "text": "The organization defines the maximum number of consecutive invalid logon attempts to the information system by a user during an organization-defined time period."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-7",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000044",
              "shortDescription": {
                "text": "The information system enforces the organization-defined limit of consecutive invalid logon attempts by a user during the organization-defined time period."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-7",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000045",
              "shortDescription": {
                "text": "The organization defines in the security plan, explicitly or by reference, the time period for lock out mode or delay period."
              }
            },
            {
              "id": "CCI-000046",
              "shortDescription": {
                "text": "The organization selects either a lock out mode for the organization-defined time period or delays the next login prompt for the organization-defined delay period for information system responses to consecutive invalid access attempts."
              }
            },
            {
              "id": "CCI-000047",
              "shortDescription": {
                "text": "The information system delays next login prompt according to the organization-defined delay algorithm, when the maximum number of unsuccessful attempts is exceeded, automatically locks the account/node for an organization-defined time period or locks the account/node until released by an Administrator IAW organizational policy."
              }
            },
            {
              "id": "CCI-000048",
              "shortDescription": {
                "text": "The information system displays an organization-defined system use notification message or banner before granting access to the system that provides privacy and security notices consistent with applicable federal laws, Executive Orders, directives, policies, regulations, standards, and guidance."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-8",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000049",
              "shortDescription": {
                "text": "The organization defines a system use notification message or banner displayed before granting access to the system that provides privacy and security notices consistent with applicable federal laws, Executive Orders, directives, policies, regulations, standards, and guidance and states that: (i) users are accessing a U.S. Government information system; (ii) system usage may be monitored, recorded, and subject to audit; (iii) unauthorized use of the system is prohibited and subject to criminal and civil penalties; and (iv) use of the system indicates consent to monitoring and recording."
              }
            },
            {
              "id": "CCI-000050",
              "shortDescription": {
                "text": "The information system retains the notification message or banner on the screen until users acknowledge the usage conditions and take explicit actions to log on to or further access the information system."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-8",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000051",
              "shortDescription": {
                "text": "The organization approves the information system use notification message before its use."
              }
            },
            {
              "id": "CCI-000052",
              "shortDescription": {
                "text": "The information system notifies the user, upon successful logon (access) to the system, of the date and time of the last logon (access)."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-9",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000053",
              "shortDescription": {
                "text": "The information system notifies the user, upon successful logon/access, of the number of unsuccessful logon/access attempts since the last successful logon/access."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-9",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000054",
              "shortDescription": {
                "text": "The information system limits the number of concurrent sessions for each organization-defined account and/or account type to an organization-defined number of sessions."
              },
              "relationships": [
                {
                  "target": {
                    "id": "AC-10",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
              "id": "CCI-000055",
              "shortDescription": {
