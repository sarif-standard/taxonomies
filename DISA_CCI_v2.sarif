              },
              "relationships": [
                {
                  "target": {
                    "id": "PM-6",
                    "toolComponent": {
                      "name": "NIST",
                      "guid": "EE4E6942-6346-45EE-BBA2-9998A214D80E"
                    }
                  },
                  "kinds": [
                    "relevant"
                  ]
                }
              ]
            },
            {
